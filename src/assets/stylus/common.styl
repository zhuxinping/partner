.container
	background-color #f2f3f5
	-webkit-overflow-scrolling touch
	position absolute
	z-index 0
	width 100%
	height 100%
	display flex
	flex-direction column
	.flex
		background-color #f2f3f5
	.right-text
		font-size 30px
		color #FFFFFF
		text-align center
		position absolute
		right 40px
		top 22px
	.right-img
		width 36px
		height 36px
		position absolute
		padding-left 80px
		padding-right 30px
		padding-top 24px
		display block
		top 0
		right 0
	.nav-status
		width 100%;
		height 88px
		list-style 88px
		display flex
		align-items center
		background-color #fff
		border-bottom 2.1px solid #eee
		list-style none
		justify-content space-around
		.status
			font-size 30px
			color #749AFF
			.bg-color
				height 60px
				line-height 72px
				display inline-block
				color #74A9FF
				opacity 0.5
			.change-color
				opacity 1
				border-bottom 4px solid #7CA8EF
	.search
		width 100%
		height 88px
		display flex
		align-items center
		justify-content space-between
		background-color #FFF
		.search_school
			width 94%
			align-items center
			margin-left 3%
			display flex
			background-color #F2F3F5
			height 70px
			border-radius 200px
			img
				width 32px
				height 32px
				display block
				margin 0 30px
			input
				border 0
				width 85%
				font-size 30px
				background-color #f2f3f5
	.strategy
		width 100%
		height 80px
		line-height 80px
		background #FFF6CF
		img,span
			float left
		.tip
			height 30px
			margin-left 20px
			margin-top 25px
			margin-right 2.7%
		span
			font-size 28px
			color #CB730C
			text-align left
			width 80%
		.tip_close
			width 23.4px
			height 23.4px
			margin-top 28.3px
			float right
			margin-right 4.2%
	.main
		flex 1
		background-color #f2f3f5
		overflow-y auto
		z-index 10
		.report-main, .main-list
			display flex
			padding 20px
			align-items center
			border-bottom 2.1px solid #eee
	.alert_ul
		display flex
		flex-direction column
		padding 30px
		padding-bottom 10px
		.next_router
			height 100px
			line-height 100px
			font-size 32px
			color #333333
			text-align center
			border-bottom 2.1px solid #eeeeee
		.next_router.li
			overflow hidden
		.title
			font-size 34px
			color #333333
			font-weight bold
		.example
			font-size 26px
			color #151515
			padding-bottom 20px
			text-align left
			.raito
				color #74A9FF
				margin-bottom 10px
				span
					color #151515
					margin-left 10px
			.roito-left
				max-width 470px
				span
					font-size 26px
					padding-left 8px
					padding-right 8px
		.message p
			font-size 26px
			padding: 0 20px
			color #151515
			text-align left
			margin-bottom 10px
		ul.foot
			margin-top 30px
			display flex
			border-top 2.1px solid #eee
			width 100%
			height 80px
			align-items center
			font-size 30px
			color #74A9FF
		ul.foot li
			flex 1
		ul.foot li:nth-child(1):after
			content ''
			width 2.1px
			height 80px
			position absolute
			left 50%
			bottom 0
			background #eee
		.last-alert
			color #74A9FF
			font-size 30px
			height 88px
			line-height 88px
			text-align center
			border-top 2.1px solid #eeeeee
	.visited-text
		display flex
		flex-direction column
		.alert-logo
			font-size 36px
			color #333
			margin auto
			margin-top 30px
			margin-bottom 20px
			position relative
			img
				display inline-block
				width 25px
				height 25px
				position absolute
				top 0
				left 280px
		.alert-content
			font-size 28px
			color #818D9D
			max-height 400px
			overflow-y auto
			text-align left
			padding 0 20px 50px
			word-break break-all
	.main-bg
		background-color #fff
		.sd-title
			color #74A9FF
			font-size 30px
			text-align left
			padding 0 30px
			margin-top 30px
		p
			padding 0 30px
			font-size 28px
			color #333333
			line-height 48px
			text-align left
		.sd-table
			margin 20px 30px
			border 1px solid #eee
			border-bottom 0
			li
				display flex
				flex-direction row
				border-bottom 1px solid #eee
				span
					display flex
					flex 1
					padding 20px 10px
					align-items center
					justify-content center
					font-size 28px
					color #333
					border-right 1px solid #eee
				span:last-child
					border-right 0
			.sd-thead
				border 1px solid #eee
				background-color #f3fbff
		.sd-text
			font-size 26px
			color  #818D9D
			height 88px
			line-height 88px
			background-color #f2f3f5
i.common
	width: 92px
	height: 42px
	text-align center
	display inline-block
	vertical-align middle
	font-size 40px
	color #333
	font-style normal
// 标签
i.rank1
	background url("../../assets/images/icon_jp.png") no-repeat center
	background-size 32px 42px
i.rank2
	background url("../../assets/images/icon_yp.png") no-repeat center
	background-size 32px 42px
i.rank3
	background url("../../assets/images/icon_tp.png") no-repeat center
	background-size 32px 42px
i.icon-answer
	width: 32px
	height: 32px
	background url("../../assets/images/operate-explain.png") no-repeat center
	background-size 100% 100%
	margin-left 14px
	display inline-block
i.icon-wenhao
	width: 28px
	height: 28px
	background url("../../assets/images/icon-wenhao.png") no-repeat center
	background-size 100% 100%
	margin-left 14px
	margin-right 30px
	display inline-block
i.icon-right{
	width: 12px
	height: 20px
	margin-right 30px
	background url("../../assets/images/icon_arrow.png") no-repeat
	background-size 100% 100%
}
i.icon-right-red{
	width: 12px
	height: 20px
	margin-right 30px
	background url("../../assets/images/icon_arrow_red.png") no-repeat
	background-size 100% 100%
}
i.icon-down
	width 17px
	height 30px
	display block
	background-size 100% 100%
	margin-left 10px
	background-image url("../../assets/images/icon_down.png")
i.icon-up
	width 17px
	height 30px
	display block
	background-size 100% 100%
	margin-left 10px
	background-image url("../../assets/images/icon_up.png")
i.icon-select
	width: 20px
	height: 12px
	background url("../../assets/images/icon_time_down.png") no-repeat
	background-size 100% 100%
	display inline-block
i.icon-add
	width 18px
	height 18px
	background url("../../assets/images/icon_add.png") no-repeat center
	background-size 100% 100%
	display inline-block
	vertical-align baseline
i.icon-reduce
	width 18px
	height 3px
	background url("../../assets/images/icon_reduce.png") no-repeat center
	background-size 100% 100%
	display inline-block
	vertical-align middle
.spr-title
	width 100%
	display flex
	height 82px
	line-height 82px
	align-items center
	justify-content space-between
	background-color #fff
.spr-class
	width 400px
	font-size 26px
	color #00AAFF
	text-align right
	margin-right 20px
	.icon-arrow
		display inline-block
		margin-left 12px
		width 11px
		height 20px
		background url('../../assets/images/icon-arrow_blue.png')
		background-size 100% 100%
.spr-left
	margin-left 20px
	flex 1
	height 82px
	font-size 30px
	color #333333
	display flex
	align-items center
	em
		position relative
		padding-left 20px
	em:after
		position absolute
		background #05ACFF
		content ''
		height 34px
		width 6px
		top 50%
		left 0
		margin-top -17px
.tab-db .chart-line
	width 100%
	height 418px
.tab
	display flex
	background #FFFFFF
	border 2.1px solid #74A9FF
	border-radius 10px
	margin-right 20px
	li
		width 120px
		height 50px
		line-height 50px
		font-size 22px
		color #74a9ff
		text-align center
	.active
		background #74a9ff
		color #fff
::-webkit-input-placeholder /* WebKit, Blink, Edge */
	color #BBBBBD
:-moz-placeholder /* Mozilla Firefox 4 to 18 */
	color #BBBBBD
::-moz-placeholder /* Mozilla Firefox 19+ */
	color #BBBBBD
:-ms-input-placeholder/* Internet Explorer 10-11 */
	color #BBBBBD
