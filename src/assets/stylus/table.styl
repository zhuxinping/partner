.from-table
	width 100%
	.table-li-no-height
		background-color #fff
		display flex
		align-items center
		justify-content space-between
		border-bottom 2.1px solid #eee
	.table-li
		background-color #fff
		display flex
		height 88px
		align-items center
		line-height 88px
		justify-content space-between
		border-bottom 2.1px solid #eee
		.required
			width 15px
			height 15px
			display block
			margin-left 30px
		.key-content
			font-size 32px
			color #333333
			width 240px
			margin-left 20px
			text-align left
		.no-require
			margin-left 30px
		.value-content
			color #818d9d
			font-size 28px
			flex 1
			padding-right 30px
			display block
			border 0
			text-align right
			overflow hidden
			text-overflow ellipsis
			white-space nowrap
		.tel
			width 36px
			height 36px
			margin-right 30px
			display block
		.back
			width 11px
			height 20px
			margin-right 30px
			display block
		.value-location
			width 91%
			margin-left 20px
			display flex
			.open-map
				width 24px
				height 32px
			.location-content
				width 90%
				margin-left 20px
				text-align left
				flex-direction row
		.map
			width 24px
			height 32px
			margin-right 30px
			display block
	.business
		display flex
		flex-direction column
		background-color #fff
		border-bottom 2.1px solid #eee
		.bussiness-top
			display flex
			.required
				width 15px
				height 15px
				display block
				margin 35px 0 0 30px
			.key-content
				font-size 32px
				color #333333
				flex 1
				margin 20px 0 0 20px
				text-align left
		.business-bg
			overflow hidden
			.img-bg
				display block
				width 140px
				height 140px
				margin 25px 0 25px 20px
				object-fit cover
	.table-textarea
		height 300px
		flex-direction column
		.key-top
			display flex
			flex-direction row
			width 100%
			height 88px
			align-items center
			line-height 88px
			.key-span
				margin-left 30px
			.no-require
				margin-left 30px
		.value-textarea
			width 100%
			height 170px
			position relative
			top -60px
			textarea
				width 94%
				height 160px
				font-size 28px
				resize none
				border 0
				margin-left 20px
				line-height 40px
				color #818d9d
			.count
				font-size 24px
				color #bbbbbd
				position absolute
				right 30px
				top 150px
	.table-camera
		position relative
		height 210px
	.back-type
		background-color #ffffff
		border-bottom 2.1px solid #eee
		padding-left 30px
		padding-bottom 20px
		overflow hidden
		.required
			width 15px
			height 15px
			display block
			margin-top 34px
			float left
		.key-content
			font-size 32px
			color #333333
			width 145px
			margin-left 20px
			margin-top 20px
			text-align left
			float left
		.value-type
			float left
			width 160px
			height 50px
			line-height 50px
			text-align center
			background-color #f2f3f5
			margin-right 20px
			font-size 24px
			color #818D9D
			border-radius 8px
			margin-top 20px
			overflow hidden
			text-overflow ellipsis
			white-space nowrap
		.value-bug
			width 170px
			height 74px
			background-color #f2f3f5
			margin-right 20px
			line-height 35px
			padding 0 20px
			font-size 24px
			color #818D9D
			border-radius 8px
			margin-top 20px
			align-items center
			justify-content center
			word-break break-all
			overflow hidden
			text-overflow ellipsis
			word-wrap break-word
			display -webkit-box
			-webkit-line-clamp 2
			-webkit-box-orient vertical
		.value-time
			width 140px
			height 54px
			line-height 54px
			display flex
			background-color #f2f3f5
			justify-content center
			margin-right 20px
			font-size 24px
			color #818D9D
			border-radius 8px
			margin-top 20px
			align-items center
		.bug-content
			display flex
			flex 1
			flex-wrap wrap
			justify-content flex-end
			margin-right 10px
		.click-span
			background #E5F6FF
			color #00AAFF
	.children-select
		background-color #fff
		border-bottom 2.1px solid #eee
		overflow hidden
		.children-li
			line-height 80px
			float right
			position relative
			margin-right 20px
			span.checkcont
				float: left
				margin-left 20px
				margin-top 24px
				width 36px
				height 36px
				box-sizing: border-box
				background-image url("../../assets/images/icon-square-unselected.png")
				background-size 100% 100%
				padding-right 16px
				input
					display none
				input[type=checkbox]:checked + label
					border none
					background: url(../../assets/images/icon-square-elected.png) no-repeat;
					background-size 100% 100%
				label
					width 36px
					height 36px
					float left
					display inline-block
					box-sizing border-box
					position absolute
					top 24px
					left 20px
				label.disabled
					cursor not-allowed
				label.disabled::after
					content: ''
			.span
				float right
				font-size 28px
				margin-top 3px
				margin-left 10px
				overflow hidden
				text-overflow ellipsis
				white-space nowrap
				color #818D9D
.table-button
	width 100%
	height 88px
	align-items center
	line-height 88px
	display flex
	margin-top 40px
	background-color #fff
	li
		flex 1
		font-size 32px
	.from
		background-color #7CA8EF
		color #ffffff
