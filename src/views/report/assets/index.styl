.waitting
	width 100%
	background-color #f2f3f5
	color: #818d9d;
	height: 65px;
	line-height: 65px;
	padding-left: 25px;
	box-sizing: border-box;
	font-size: 28px;
	text-align: left ;
	span
		margin-right: 30px
		float: left
.report-main
	background-color #fff
	flex-direction row
	.left-img
		width: 160px;
		height: 160px;
		margin-right 20px
	dd
		flex 1
		position relative
		color: #818D9D;
		font-size:28px;
		p
			text-overflow:ellipsis;
			white-space:nowrap
			overflow: hidden;
			width 60%
			height 50px
			line-height 50px
			text-align left
		.schoolName
			font-size: 32px;
			color:#333333;
		.class
			text-align left
			span
				margin-right: 20px
		.icon-right
			position: absolute;
			right: 20px;
			top: 45%;
		.right-top
			width: 70px;
			height: 70px;
			position: absolute;
			right: -20px;
			top: -30px;
